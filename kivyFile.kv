<ContentNavigationDrawer>:
    orientation: "vertical"
    padding: "0dp", "220dp"
    spacing: "30dp"
    md_bg_color: 1,1,1,1
    ScrollView:
        MDList:
            OneLineListItem:
                text: "Help"
                on_release: app.help()
            OneLineListItem:
                text: "About"
                on_release: app.openAbout()
            OneLineListItem:
                text: "Settings"
                on_release: app.settings()
            OneLineListItem:
                text: "Quit"
                on_release: app.stop()
            


